{
  "name": "portfolio-miniapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:with-sourcemaps": "tsc && vite build --sourcemap",
    "preview": "vite preview --port 5173 --strictPort",
    "typecheck": "tsc --noEmit",
    "test": "vitest run src/__tests__",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "playwright:install": "playwright install --with-deps",
    "test:a11y": "vitest run src/__tests__/a11y.spec.ts",
    "lhci": "lhci autorun || echo 'LHCI warnings'",
    "bundle:report": "node scripts/bundle-report.js"
  },
  "dependencies": {
    "@sentry/browser": "^8.27.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.22.3",
    "web-vitals": "^4.2.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.44.1",
    "@redocly/cli": "^1.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/telegram-web-app": "file:src/types/telegram-web-app",
    "@vitejs/plugin-react": "^5.0.3",
    "axe-core": "^4.10.0",
    "jsdom": "^24.0.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.0",
    "vitest-axe": "^0.1.0"
  }
}
